<!DOCTYPE html>{% set tapir_mostrarmodal = 0 %}{% set block_pagina_titulo = block('pagina_titulo') %}  
<html lang="es">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="{{ tapir_nombreaplicacion }}">
    <meta name="author" content="Ernesto N. Carrea">

    <title>{{ tapir_nombreaplicacion }}{% if block_pagina_titulo is not empty %} - {{ block_pagina_titulo|raw }}{% endif %}</title>

    {% image '@tapir_favicon' %}
        <link rel="icon" href="{{ asset_url }}">
    {% endimage %}
    
    <script type="text/javascript" src="{{ asset('bundles/tapirbase/js/jquery-2.1.1.min.js') }}"></script>
    
{% block stylesheets %}
    {% stylesheets '@TapirBaseBundle/Resources/less/tapir.less' %}
    <link href="{{ asset_url }}" type="text/css" rel="stylesheet" />
    {% endstylesheets %}
{% endblock %}

    <link href="{{ asset('bundles/tapirbase/css/font-awesome.min.css') }}" rel="stylesheet" />
    <disabled-link href="{{ asset('bundles/tapirbase/css/bootstrap.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/tapirbase/css/tapir.css') }}" rel="stylesheet" />
    
    <disabled-script type="text/javascript" src="{{ asset('bundles/tapirbase/js/modernizr.custom.85769.js') }}"></disabled-script>
</head>

<body>
    {% for type, flashMessages in app.session.flashbag.all() %}
    {% for flashMessage in flashMessages %}
        <div class="alert alert-dismissable alert-{{ type }}">
            <button type="button" class="close" data-dismiss="alert">Ã—</button>
            {{ flashMessage|raw }}
        </div>
    {% endfor %}
    {% endfor %}

    {% block pagina_contenido %}{% endblock %}

    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="{{ asset('@TapirBaseBundle/Resources/public/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('@TapirBaseBundle/Resources/public/js/jquery.history.js') }}"></script>
    <script type="text/javascript" src="{{ asset('@TapirBaseBundle/Resources/public/js/tapir.js') }}"></script>
    {% if app.environment != 'test' %}
        <script type="text/javascript" src="{{ asset('@TapirBaseBundle/Resources/public/js/tapir-ajax.js') }}"></script>
    {% endif %}
  </body>
</html>