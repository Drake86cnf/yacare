{% extends 'YacareBaseBundle:Default:listar.html.twig' %}

{% block pagina_titulo %}Vehiculo{% endblock %}



{% block pagina_contenido %}

{% include 'YacareBaseBundle::buscar.html.twig' %}

<table class="table table-striped">
    <thead>
        <tr>
            <th>Dominio</th>
            <th>Marca</th>
            <th>Modelo</th>
            <th>Ano</th>
            <th>Peso</th> 
            <th>Transporte</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('yacare_bromatologia_vehiculo_editar', { 'id': entity.Id }) }}"><i class="fa fa-edit"></i></a> {{ entity.Dominio }}</td>
            <td>{{ entity.Marca }}</td>
            <td>{{ entity.Modelo }}</td>
            <td>{{ entity.Ano }}</td>
            <td>{{ entity.Peso }}</td>
            <td>{{ entity.Transporte }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{# display navigation #}
<div class="navigation">
    {{ knp_pagination_render(entities) }}
</div>

<a href="{{ path('yacare_bromatologia_vehiculo_editar_1') }}" class="btn btn-primary"><i class="fa fa-plus"></i> Nuevo veh√≠culo</a>
{% endblock %}
