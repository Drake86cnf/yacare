{% extends 'YacareBaseBundle:Default:listar.html.twig' %}

{% block pagina_titulo %}Tipos de trámite{% endblock %}

{% block pagina_contenido %}
{% include 'YacareBaseBundle::buscar.html.twig' %}

<table class="table table-hover">
    <thead>
        <tr>
            <th> </th>
            <th>Nombre</th>
            <th>Web</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td>
                <a data-toggle="ajax-link" class="btn btn-default btn-xs hidden-print" href="{{ path(baseroute ~ '_ver', { 'id': entity.Id }) }}"><i class="fa fa-folder-open-o"></i> Ver</a>
            </td>
            <td>{{ entity.Nombre }}</td>
            <td>{{ entity.Url }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% include 'YacareBaseBundle::paginar.html.twig' %}
{% endblock %}

{% block pagina_acciones2 %}
<a data-toggle="ajax-link" href="{{ path(baseroute ~ '_editar_1', arrastre) }}" class="btn btn-primary"><i class="fa fa-plus"></i> Nuevo tipo de trámite</a>
{% endblock %}