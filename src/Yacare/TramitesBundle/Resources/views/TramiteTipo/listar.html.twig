{% extends 'YacareBaseBundle:Default:listar.html.twig' %}

{% block title %}Tipos de trámite{% endblock %}

{% block content_header '' %}

{% block content %}

<h1>Tipos de trámite</h1>
{% include 'YacareBaseBundle::buscar.html.twig' %}

<table class="table table-hover">
    <thead>
        <tr>
            <th> </th>
            <th>Nombre</th>
            <th>Web</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a class="btn btn-default btn-xs" href="{{ path(baseroute ~ '_editar', { 'id': entity.Id }) }}"><i class="fa fa-edit"></i> Editar</a>
                <a class="btn btn-default btn-xs" href="{{ path('yacare_tramites_tramitetiporequisito_listar', { 'parent_id': entity.Id }) }}"><i class="fa fa-check"></i> Ver requisitos</a></td>
            <td>{{ entity.Nombre }}</td>
            <td>{{ entity.Url }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% include 'YacareBaseBundle::paginar.html.twig' %}

<a href="{{ path('yacare_tramites_tramitetipo_editar_1', arrastre) }}" class="btn btn-primary"><i class="fa fa-plus"></i> Nuevo tipo de trámite</a>
{% endblock %}
