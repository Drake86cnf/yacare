{% extends 'TapirBaseBundle:Default:listar.html.twig' %}

{% block pagina_titulo %}Tipos de trámite{% endblock %}

{% block pagina_contenido %}
{% include 'TapirBaseBundle::buscar.html.twig' %}

<table class="table table-hoverselect">
    <thead>
        <tr>
            <th>Nombre</th>
            <th class="hidden-sm hidden-xs">Web</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr data-toggle="ajax-link" href="{{ path(baseroute ~ '_ver', { 'id': entity.Id }) }}">
            <td>{{ entity.Nombre }}</td>
            <td class="hidden-sm hidden-xs">{{ entity.Url }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{% include 'TapirBaseBundle::paginar.html.twig' %}
{% endblock %}

{% block pagina_acciones2 %}
<a data-toggle="ajax-link" href="{{ path(baseroute ~ '_editar_1', arrastre) }}" class="btn btn-primary"><i class="fa fa-plus"></i> Nuevo tipo de trámite</a>
{% endblock %}