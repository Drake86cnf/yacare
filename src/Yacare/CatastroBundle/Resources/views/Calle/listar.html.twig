{% extends 'YacareBaseBundle:Default:listar.html.twig' %}

{% block title %}Calles{% endblock %}

{% block content_header '' %}

{% block content %}
<table class="table table-striped">
    <thead>
        <tr>
            <th>Nombre</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('yacare_catastro_calle_editar', { 'id': entity.Id }) }}">{{ entity.Nombre }}</a></td>
        </tr>
    {% endfor %}
    </tbody>
</table>

{# display navigation #}
<div class="navigation">
    {{ knp_pagination_render(entities) }}
</div>
{% endblock %}