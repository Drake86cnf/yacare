{% set yacare_mostrarmodal = 0 %}
{% set block_pagina_titulo = block('pagina_titulo') %}  
<!DOCTYPE html>
<html lang="es">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Sistema de gestión municipal Yacaré">
    <meta name="author" content="Ernesto N. Carrea">

    <title>Yacaré{% if block_pagina_titulo is not empty %} - {{ block_pagina_titulo|raw }}{% endif %}</title>

    <link rel="icon" href="{{ asset('bundles/yacarebase/img/yacare_logo64.png') }}">
    
    <link href="{{ asset('bundles/yacarebase/css/bootstrap.min.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/yacarebase/css/font-awesome.min.css') }}" rel="stylesheet" />

    <link href="{{ asset('bundles/yacarebase/css/sb-admin.css') }}" rel="stylesheet" />
    <link href="{{ asset('bundles/yacarebase/css/yacare.css') }}" rel="stylesheet" />
    
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/modernizr.custom.85769.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/jquery-2.1.0.min.js') }}"></script>
  </head>

  <body>

    <div id="wrapper">

      <!-- Opción de accesibilidad para saltear el menú de navegación -->
      <a href="#page-wrapper" class="sr-only">Ir al contenido principal</a>
      
      <!-- Sidebar -->
      <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <!-- Brand and toggle get grouped for better mobile display -->
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-ex1-collapse">
            <span class="sr-only">Mostrar u ocultar el menú de navegación</span>
            <span class="fa fa-ellipsis-v"></span>
          </button>
          <a class="navbar-brand" href="{{ path('inicio') }}"><img src="{{ asset('bundles/yacarebase/img/yacare_logo_gris20.png') }}" alt="Yacaré" /> Yacaré</a>
          <span class="navbar-slogan">Municipio de Río Grande</span>
        </div>

        <!-- Collect the nav links, forms, and other content for toggling -->
        <div role="navigation" id="menuprincipal" class="collapse navbar-collapse navbar-ex1-collapse">
          {% include 'YacareBaseBundle::menu.html.twig' %}
        </div><!-- /.navbar-collapse -->
      </nav>

      <div id="page-wrapper">
        {% for type, flashMessages in app.session.flashbag.all() %}
        {% for flashMessage in flashMessages %}
            <div class="alert alert-dismissable alert-{{ type }}">
                <button type="button" class="close" data-dismiss="alert">×</button>
                {{ flashMessage|raw }}
            </div>
        {% endfor %}
        {% endfor %}

        {% block pagina_inicio %}{% endblock %}

        {% if block_pagina_titulo is not empty %}
            <h1 id="page-title">{{ block_pagina_titulo|raw }}</h1>
        {% endif %}

        {% block pagina_contenido %}{% endblock %}
        {% block pagina_pie %}{% endblock %}
        
        {% set block_pagina_acciones = block('pagina_acciones') %}
        {% if block_pagina_acciones is not empty %}
            <div class="form-actions hidden-print">
                {{ block_pagina_acciones|raw }}
            </div>
        {% endif %}
        
        {% set block_pagina_acciones2 = block('pagina_acciones2') %}
        {% if block_pagina_acciones2 is not empty %}
            <div class="form-actions2 hidden-print">
                {{ block_pagina_acciones2|raw }}
            </div>
        {% endif %}
            
        {% set block_pagina_acciones3 = block('pagina_acciones3') %}
        {% if block_pagina_acciones3 is not empty %}
            <div class="form-actions3 hidden-print">
                {{ block_pagina_acciones3|raw }}
            </div>
        {% endif %}

        {% block pagina_fin %}{% endblock %}

      </div><!-- /#page-wrapper -->

    </div><!-- /#wrapper -->

    <div class="modal hidden-print" id="modal" role="dialog">
        Cargando...
    </div>

    <!-- Bootstrap core JavaScript -->
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/jquery-ui-1.10.4.custom.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/jquery.ui.datepicker-es.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/bootstrap.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/jquery.history.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/tinymce/tinymce.min.js') }}"></script>
    <script type="text/javascript" src="{{ asset('bundles/yacarebase/js/yacare.js') }}"></script>
   
<script type="text/javascript">
    tinymce.init({
        selector: '.tinymce',
        language: 'es',
        spellchecker_languages: '+Spanish=sp',
        content_css: '{{ asset('bundles/yacarebase/css/yacare.css') }}',
        plugins: 'link paste spellchecker textcolor',
        toolbar1: 'bold italic underline strikethrough | justifyleft justifycenter justifyright justifyfull | forecolor backcolor | removeformat | sub sup | undo redo spellchecker',
        toolbar2: "",
        theme_modern_toolbar_location: 'top',
        theme_modern_toolbar_align: 'left',
        menubar: false,
        statusbar: false,
        paste_auto_cleanup_on_paste: true,
        use_callback_tinymce_init: false,
        jquery_script_url: '{{ asset('bundles/yacarebase/js/jquery-2.1.0.min.js') }}'
     });
</script>
{# tinymce_init() #}
    
    {% block pagina_scripts %}{% endblock %}
  </body>
</html>